    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run Main Script",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/aquabuddy/main.py",
            "console": "integratedTerminal",
            "args": []
        },
        {
            "name": "Run FileDump Gui",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/llv/filedump.py",
            "console": "integratedTerminal",
            "args": []
        },
        {
            "name": "Debug FileDump Gui",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/llv/filedump.py",
            "console": "integratedTerminal"
        },
    ]
}